{
  "apps": [
    {
      "name": "socketing-server",
      "cwd": "./",
      "script": "./dist/main.js",
      "instances": "max",
      "exec_mode": "cluster",
      "merge_logs": true,
      "autorestart": true,
      "watch": false,
      "wait_ready": true,
      "listen_timeout": 50000,
      "kill_timeout": 5000,
      "instance_var": "PM2_INSTANCE_ID",
      "env_local": {
        "NODE_ENV": "local"
      },
      "env_dev": {
        "NODE_ENV": "dev"
      },
      "env_production": {
        "NODE_ENV": "production"
      }
    }
  ]
}
